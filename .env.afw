# AFW Project Environment Variables
PROJECT_NAME=afw
COMPOSE_PROJECT_NAME=afw

# Ports - UNIQUE to AFW project
AFW_FRONTEND_PORT=3002
AFW_BACKEND_PORT=8002
AFW_REDIS_PORT=6380

# Domain
AFW_DOMAIN=afw-app.duckdns.org

# Backend Configuration
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
HOST=0.0.0.0
PORT=8001
PYTHONPATH=/app
ENVIRONMENT=production
DEBUG=false

# Security Keys - AFW specific
JWT_SECRET_KEY=afw_secret_key_2024_unique
JWT_ALGORITHM=HS256
SESSION_SECRET=afw_session_secret_2024_unique
ENCRYPTION_KEY=afw_encryption_key_2024_unique
API_SECRET_KEY=afw_api_secret_2024_unique
REDIS_KEY=afw_redis_key_2024_unique

# Redis Configuration
REDIS_URL=redis://afw-redis:6379
REDIS_HOST=afw-redis
REDIS_PORT=6379

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://afw-app.duckdns.org
NEXT_PUBLIC_VERSION=1.0.0
NEXT_PUBLIC_DEMO_MODE=false
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Rate Limiting
RATE_LIMIT_ENABLED=true
CORS_MAX_AGE=86400

# API Keys (add your keys here)
GROQ_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
